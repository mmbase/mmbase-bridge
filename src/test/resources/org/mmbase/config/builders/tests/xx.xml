<?xml version="1.0" encoding="UTF-8"?>
<builder
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder
                        http://www.mmbase.org/xmlns/builder.xsd"
    name="xx" maintainer="mmbase.org" version="0" extends="object">
  <names>
    <singular xml:lang="nl">xx</singular>
    <singular xml:lang="fr">xx</singular>
    <singular xml:lang="en">xx</singular>
    <plural xml:lang="nl">xx's</plural>
    <plural xml:lang="fr">xxs</plural>
    <plural xml:lang="en">xxs</plural>
  </names>

  <descriptions>
    <description xml:lang="nl">Builder voor het testen van de bridge classes, met 'notnull' velden</description>
    <description xml:lang="en">Builder to test the bridge classes, with notnull fields</description>
  </descriptions>

  <fieldlist>
    <field>
      <gui>
        <guiname xml:lang="nl">Naam</guiname>
        <guiname xml:lang="fr">Nom</guiname>
        <guiname xml:lang="en">Name</guiname>
      </gui>
      <editor>
        <positions>
          <list>1</list>
        </positions>
      </editor>
      <db>
        <name>name</name>
        <type key="false" notnull="true" size="255" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guiname xml:lang="fr">Description</guiname>
        <guiname xml:lang="en">Description</guiname>
      </gui>
      <db>
        <name>description</name>
        <type key="false" notnull="true" size="2048" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">BinaryField</guiname>
      </gui>
      <editor>
        <positions>
          <list>2</list>
        </positions>
      </editor>
      <db>
        <name>binaryfield</name>
        <type key="false" notnull="true" state="persistent">BYTE</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">DoubleField</guiname>
      </gui>
      <db>
        <name>doublefield</name>
        <type key="false" notnull="true" state="persistent">DOUBLE</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">FloatField</guiname>
      </gui>
      <db>
        <name>floatfield</name>
        <type key="false" notnull="true" state="persistent">FLOAT</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">IntField</guiname>
      </gui>
      <db>
        <name>intfield</name>
        <type key="false" notnull="true" state="persistent">INTEGER</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">LongField</guiname>
      </gui>
      <db>
        <name>longfield</name>
        <type key="false" notnull="true" state="persistent">LONG</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">StringField</guiname>
      </gui>
      <db>
        <name>stringfield</name>
        <type key="false" notnull="true" size="2048" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">XmlField</guiname>
      </gui>
      <db>
        <name>xmlfield</name>
        <type key="false" notnull="true" size="2048" state="persistent">XML</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">NodeField</guiname>
      </gui>
      <db>
        <name>nodefield</name>
        <type key="false" notnull="true" state="persistent">NODE</type>
      </db>
    </field>


    <field>
      <gui>
        <guiname xml:lang="en">DateTimeField</guiname>
      </gui>
      <db>
        <name>datetimefield</name>
        <type key="false" notnull="true" state="persistent">DATETIME</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="en">BooleanField</guiname>
      </gui>
      <db>
        <name>booleanfield</name>
        <type key="false" notnull="true" state="persistent">BOOLEAN</type>
      </db>
    </field>
    <field>
      <db>
        <name>decimalfield</name>
        <type key="false" notnull="true" state="persistent">DECIMAL</type>
      </db>
    </field>

<!--
    <field>
      <gui>
        <guiname xml:lang="en">ListField</guiname>
      </gui>
      <db>
        <name>listfield</name>
        <type key="false" notnull="true" state="persistent">LIST[BOOLEAN]</type>
      </db>
    </field>
    -->

  </fieldlist>

</builder>
