<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder
    name="legacy_types" maintainer="mmbase.org" version="0" extends="object">

  <fieldlist>
    <field>
      <!-- e.g. for     MMB-1248 -->
      <gui>
        <guiname />
        <guitype>eventtime</guitype>
      </gui>
      <editor>
        <positions>
          <list>1</list>
        </positions>
      </editor>
      <db>
        <name>eventtime</name>
        <type state="persistent" notnull="false" key="false">INTEGER</type>
      </db>
    </field>

    <field>
      <!-- e.g. for         MMB-1283  -->
      <gui>
        <guiname />
        <guitype>boolean</guitype>
      </gui>
      <editor>
        <positions>
          <list>2</list>
        </positions>
      </editor>
      <db>
        <name>string_boolean</name>
        <type state="persistent" notnull="false" key="false">STRING</type>
      </db>
    </field>
  </fieldlist>
</builder>
